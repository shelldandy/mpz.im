---
import { getEntry } from 'astro:content';
import Homepage from '../layouts/Homepage.astro';

const homepage = await getEntry('pages', 'index');
const { Content } = await homepage.render();
const { title, hero, cta, description, ogImage } = homepage.data;
---

<Homepage
  title={title || 'Miguel Palau'}
  hero={hero}
  cta={cta}
  description={description}
  ogImage={ogImage}
>
  <Content />
</Homepage>
